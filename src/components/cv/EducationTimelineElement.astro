---
const { Item } = Astro.props;
import TimelineElement from '@components/cv/TimelineElement.astro'

const title = Item.QualificationLevel + " in "+Item.QualificationName
const subtitle = Item.YearStart+ "-" + Item.YearEnd + " " + Item.InstituteName
---

<TimelineElement 
  title={title}
  subtitle={subtitle}
/>


